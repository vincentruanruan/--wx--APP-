<!--pages/about/index.wxml-->
<v-header logo="{{topBarLogo}}" drop="{{drop}}" rightMenu="{{rightMenu}}" paddingBox="{{true}}" />
<view class='banner'>
  <image src="{{bannerImage}}" mode="widthFix" />
</view>

<view class='desc'>2017年，成立来亿科技，专注于承接项目外包，包括APP开发、商城系统开发、社交类APP开发、微信开发、系统开发等，</view>


<view class='list'>
  <view class='top'>
    <view class='title-cn'>团队介绍</view>
    <view class='title-en'>Team Introduction</view>
  </view>
  <i-row>
    <i-col span="12" i-class="col-class" wx:for="{{teams}}" wx:key="{{index}}">
      <image class='itemIcon' src="{{item.image}}" mode='widthFix' />
      <view class='title'>{{item.name}}</view>
    </i-col>
  </i-row>
</view>

<view class='list'>
  <view class='top'>
    <view class='title-cn'>团队介绍</view>
    <view class='title-en'>Team Introduction</view>
  </view>
  <i-row>
    <i-col span="12" i-class="col-class" wx:for="{{certs}}" wx:key="{{index}}">
      <image class='itemIcon' src="{{item.image}}" mode='widthFix' />
      <view class='title'>{{item.name}}</view>
    </i-col>
  </i-row>
</view>



 <v-footer footer="{{footer}}" />