/* pages/dingzhi/index.wxss */

.banner {
  position: relative;
  height: auto;
  padding: 0;
  margin: 0;
  background: #f9f9f9;
}

.banner image {
  width: 100%;
}

.banner .title {
  position: absolute;
  top: 30%;
  left: 20px;
  width: 100%;
  color: #fff;
}

.inner{
  padding: 15px;
}

.inner view{
  margin: 10px 0;
}

.inner view.sm{
  font-size: 14px;
  color: #999;
  margin-right: 10px;
  display: inline-block;
  margin: 0;
}

.inner view.url{
  color: #108bec;
}